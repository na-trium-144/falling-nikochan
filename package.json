{
  "name": "falling-nikochan",
  "version": "8.9.0",
  "private": true,
  "type": "module",
  "scripts": {
    "chart:lint": "cd chart && eslint src/",
    "i18n:lint": "cd i18n && eslint",
    "route:lint": "cd route && eslint src/",
    "frontend:lint": "cd frontend && tsc --noEmit && eslint app/",
    "lint": "npm run chart:lint && npm run i18n:lint && npm run route:lint && npm run frontend:lint",
    "chart:t": "cd chart && tsc",
    "route:t": "cd route && tsc",
    "t": "npm run chart:t && npm run route:t",
    "prepare": "npm run t",
    "ndev": "cd frontend && next dev -H 0.0.0.0",
    "nbuild": "cd frontend && node ./woff2sfnt.js && next build",
    "vdev": "vercel dev --listen 0.0.0.0:8787",
    "ldev": "cd route/ && tsx watch serve-local.ts",
    "bdev": "cd route/ && bun run --hot serve-bun.ts"
  },
  "workspaces": [
    "chart",
    "i18n",
    "frontend",
    "route"
  ],
  "dependencies": {
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/bun": "^1.2.2",
    "@types/node": "^22.13.4",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vercel": "^41.1.4"
  },
  "overrides": {
    "typescript": "$typescript",
    "ts-node": "^10",
    "undici": "^5.28.5",
    "tar": "^7",
    "esbuild": "^0.25"
  }
}
