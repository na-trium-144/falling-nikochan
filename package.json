{
  "name": "falling-nikochan",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "tsc && npm run lint --ws --if-present",
    "format": "prettier --write .",
    "t": "npm run t --ws --if-present",
    "prepare": "npm run t --ws --if-present",
    "mongo-docker": "docker run --rm -p 27017:27017 -d mongodb/mongodb-community-server:latest",
    "ndev": "npm run dev -w frontend",
    "nbuild": "npm run build -w frontend",
    "swbuild": "npm run build -w worker",
    "ldev": "npm run dev -w route",
    "bdev": "cd route/ && bun run --hot serve-bun.ts"
  },
  "workspaces": [
    "chart",
    "i18n",
    "frontend",
    "route",
    "worker"
  ],
  "dependencies": {
    "@next/mdx": "=15.5.4",
    "next": "=15.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.38.0",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "babel-loader": "^10.0.0",
    "core-js": "^3.42.0",
    "eslint": "^9.22.0",
    "eslint-config-next": "=15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.38.0",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "overrides": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
