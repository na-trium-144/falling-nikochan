{
  "name": "falling-nikochan",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "tsc && npm run lint --ws --if-present",
    "format": "prettier --write .",
    "t": "npm run t --ws --if-present",
    "prepare": "npm run t --ws --if-present",
    "test": "npm run test --ws --if-present",
    "mongo-docker": "docker run --rm -p 27017:27017 -d mongodb/mongodb-community-server:latest",
    "ndev": "npm run dev -w frontend",
    "nbuild": "npm run build -w frontend && npm run postbuild -w frontend",
    "swbuild": "npm run build -w worker && npm run postbuild -w frontend",
    "ldev": "npm run dev -w route"
  },
  "workspaces": [
    "chart",
    "i18n",
    "frontend",
    "route",
    "worker"
  ],
  "dependencies": {
    "@next/mdx": "15.5.9",
    "next": "15.5.9",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.15.19",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-loader": "^10.0.0",
    "core-js": "^3.42.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.5.9",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^4.0.16",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-license-plugin": "^4.5.1"
  }
}
